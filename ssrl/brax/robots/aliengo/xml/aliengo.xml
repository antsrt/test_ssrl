            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.913725 0.913725 0.847059 1" mesh="trunk" />
            
            <!-- FR Leg -->
            <body name="FR_hip" pos="0.2399 -0.051 0">
